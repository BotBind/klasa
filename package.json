{
  "name": "@botbind/klasa",
  "version": "0.5.3-dev",
  "description": "Klasa framework modified for developing Bot Bind addons.",
  "homepage": "http://botbind.com",
  "bugs": {
    "url": "https://github.com/BotBind/klasa/issues"
  },
  "license": "MIT",
  "author": "BDistin",
  "main": "src/index.js",
  "types": "typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/BotBind/klasa.git"
  },
  "scripts": {
    "lint:fix": "eslint --fix src && eslint --fix --config .eslintrcmd.json --ext md guides && tslint --fix \"typings/*.ts\" && echo \"No linting errors\"",
    "lint": "eslint src && tslint \"typings/*.ts\" && eslint --ext md guides && echo \"No linting errors\"",
    "docs": "dg --source src --custom guides/.docconfig.json --output dist/docs.json --logging",
    "format:check": "prettier --check \"**/*.{ts,js,md}\" && echo \"No format errors\"",
    "format": "prettier --write \"**/*.{ts,js,md}\" && echo \"No format errors\""
  },
  "dependencies": {
    "fs-nextra": "^0.4.5",
    "prettier": "^1.18.2"
  },
  "peerDependencies": {
    "discord.js": "github:discordjs/discord.js#master"
  },
  "devDependencies": {
    "@types/node": "^12.7.2",
    "docgen": "github:dirigeants/docsgen",
    "eslint": "^6.2.1",
    "eslint-plugin-markdown": "^1.0.0",
    "tslint": "^5.19.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=10.1.0"
  }
}
